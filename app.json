{
  "expo": {
    "name": "Hackatime Monitor",
    "slug": "hackatime-monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://assets.hackclub.com/icon-progress-rounded.svg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kirubavathi.hackatimemonitor"
    },
    "web": {
      "favicon": "https://assets.hackclub.com/flag-orpheus-left.svg"
    },
    "extra": {
      "eas": {
        "projectId": "MyProjectId12345"
      }
    },
    "owner": "kirubavathi"
  }
}
